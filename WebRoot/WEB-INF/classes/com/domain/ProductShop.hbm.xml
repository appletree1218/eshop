<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.domain.ProductShop" table="product_shop">
		<composite-id>
			<key-property name="productId"/>
			<key-property name="shopId"/>
		</composite-id>
	</class>
</hibernate-mapping>